// Find Maximum number from array

#include<stdio.h>
#include<conio.h>
void main()
{
	int num[10],i,max=0,n;
	clrscr();
	printf("How many elements?\n");
	scanf("%d",&n);
	printf("Enter %d elements\n",n);
	for(i=0;i<n;i++)
	{
		scanf("%d",&num[i]);
		if(num[i]>max)
		{
			max=num[i];
		}
	}
	printf("maximum=%d",max);
	getch();
}

Output:
	How many elements?
	5
	Enter 5 elements
	25
	3
	62
	54
	100
	maximum=100
