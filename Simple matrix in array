// Simple matrix in array

#include <stdio.h> 
#include <conio.h>
 
int  main() 
{ 
	int  a[3][3];
	int  i, j;
	clrscr();

	printf( "Enter value : " );

	for( i=0 ; i<3 ; i++ )
	{
		for( j=0 ; j<3 ; j++ )
		{
			scanf( "%d", &a[ i ][ j ] );
		}
	}

	printf( "\n The Matrix is ::: \n" );
	for( i=0 ; i<3 ; i++ )
	{
		for( j=0 ; j<3 ; j++ )
		{
			printf( "%d      ", a[ i ][ j ] );
		}
		printf( "\n" );
	}

	getch();
	return 0;
}

Output:

 Enter value : 1 2 3 4 5 6 7 8 9
 The Matrix is ::: 
 1    2    3
 4    5    6
 7    8    9
