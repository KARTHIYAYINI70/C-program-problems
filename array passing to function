//array passing to function 
#include<stdio.h> 
void arrayDisplay(int arr[],int size)  //arr=2000  size=6
{
    int i; 
    for(i=0;i<size;i++) //i=0;6<6;6 
    {
        printf("%d ",arr[i]);   
    }
}
int main() 
{
    int size; 
    scanf("%d",&size);  //size=6  
    int arr[size];  //arr[6]
    int i; 
    for(i=0;i<size;i++)
    {
       scanf("%d",&arr[i]); 
    }
    arrayDisplay(arr,size);
    
}
