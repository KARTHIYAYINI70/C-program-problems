//Assignment Operator ( = )

#include <stdio.h> 
#include <conio.h>
 
int  main() 
{ 
	int a, b, c, d;
	clrscr();

	a = 10;
	b = a;
	c = a + b;
	
	d = 10;
	d = d + 10;/*  equivalent to  d += 10;  */

	printf( "\n a = %d, b = %d", a, b );
	printf( "\n c = %d, d = %d", c, d );
	
	getch();
	return 0;
}

Output:
 
 a = 10, b = 10
 c = 10, d = 10

