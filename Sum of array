// Find sum of array

#include<stdio.h>
#include<conio.h>
void main()
{
	int num[10],i,sum=0;
	clrscr();
	printf("Enter the 10 number:\n");
	for(i=0;i<10;i++)
	{
		scanf("%d",&num[i]);
		sum+=num[i];
	}
	printf("sum=%d",sum);
	getch();
}

Output:
	Enter the 10 number:
	10
	20
	30
	40
	50
	60
	70
	90
	80
	100
	sum=550
