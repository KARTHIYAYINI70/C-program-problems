#include<stdio.h> 
int main()
{
    int size; 
    scanf("%d",&size);
    int arr[size]; 
    int i; 
    for(i=0;i<size;i++)
    {
        scanf("%d",&arr[i]);
    }
    if(size%2==0)
    {
        for(i=0;i<size;i++)
        {
            arr[i]=0; 
        }
    }else 
    {
        for(i=0;i<size;i++)
        {
            arr[i]=1;
        }
    }
    for(i=0;i<size;i++)
    {
        printf("%d ",arr[i]);
    }
}
