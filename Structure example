// Simple structure example

#include<stdio.h>
#include<conio.h>
struct student
{
	int rollno;
	char name[20];
};
void main()
{
	clrscr();
	student s1={45,"Bhavin"},s2={65,"Dhwani"};
	printf("%d\t%s\n",s1.rollno,s1.name);
	printf("%d\t%s\n",s2.rollno,s2.name);
	getch();
}

Output:
	45  Bhavin
	65  Dhwani
