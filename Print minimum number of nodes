#include<stdio.h> 
int main()
{
    int a; 
    scanf("%d",&a); //678 
    int b,c,d,e,f,g,h;  
    b=a/500;   //b=1 
    c=(a%500)/100;  //c=1 
    d=((a%500)%100)/50;  //d=1  
    e=(((a%500)%100)%50)/20; //c=1  
    f=((((a%500)%100)%50)%20)/10; 
    g=(((((a%500)%100)%50)%20)%10)/5; 
    h=((((((a%500)%100)%50)%20)%10)%5)/1; 
    printf("500 notes = %d\n",b); 
    printf("100 notes = %d\n",c);
    printf("50 notes = %d\n",d); 
    printf("20 notes = %d\n",e); 
    printf("10 notes = %d\n",f)
    printf("1 notes = %d\n",h); 
}
